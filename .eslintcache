st request\r\n        const user = {\r\n          username: this.state.username,\r\n          password: this.state.password\r\n        } \r\n        \r\n        axios.post(\"http://localhost:3000/addUser/login\", user)\r\n        .then(response =>{\r\n\r\n          console.log (response, \" toooooooooooooooooooken\")\r\n          window.localStorage.setItem('token', response.data);\r\n            /// after signing the token , verify the token \r\n          axios.post(\"http://localhost:3000/addUser/verify\",  response)\r\n          .then(response =>{\r\n  \r\n            console.log (response, \"You are authenticated\")\r\n             window.location = '/AddItems'\r\n  \r\n          })\r\n          .catch(err =>alert(\"Incorrect token\"));         \r\n\r\n        })\r\n        .catch(err =>alert(\"username or password is incorrect\"));         \r\n    }\r\n\r\n\r\n    render(){\r\n        return (\r\n               <div>\r\n               <br />\r\n               <div className = \"container\">\r\n                <form className=\"text-center border border-light p-9\" onSubmit={this.onSubmit}>\r\n                <h3 className = \"mb-3\">\r\n                Login\r\n                </h3>\r\n                <br />\r\n                <div className=\"col\">\r\n                <label > User Name </label>\r\n                <br></br>\r\n                <input required = \"true\" className = \"col\" type='text' className=\"form-control\" value= {this.setState.username}onChange={this.onChangeUsername} placeholder='Enter Your User Name'/>                   \r\n                <br></br>\r\n                </div>\r\n                <br></br>\r\n                <div className=\"col\">\r\n                <label > Password </label>\r\n                <br></br>\r\n                <input required = \"true\"  className = \"col\" type=\"password\" name=\"password\" className=\"form-control col\"value= {this.setState.password} onChange={this.onChangePassword} placeholder='Enter Your