ng</h2>\r\n               \r\n                <input name=\"Search\" className=\"form-control\" onChange={e => this.onSearch(e)} value={this.state.SearchString}  placeholder=\"Search for item Name\"/>\r\n                <table className = \"table\">\r\n                <thead className = \"thead\">\r\n                    <tr>\r\n                        <th>Item Name</th>\r\n                        <th>Category</th>\r\n                        <th>Type</th>\r\n                        <th>Description</th>\r\n                        <th>Donor Phone Number</th>\r\n                        <th>Image</th>\r\n                        \r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {this.itemsList()}\r\n                </tbody>\r\n                </table>\r\n            </div>\r\n          \r\n            <Footer />\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default withRouter(ItemsList)","C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\Login.js",["68","69"],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\Signup.js",[],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\edit.js",[],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\AddItems.js",["70"],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\firebase.js",[],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\Carousel.js",[],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\Cards.js",[],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\Search.js",[],"C:\\Users\\Discovery\\Desktop\\Dafah\\src\\components\\Footer.js",[],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":4,"column":27,"nodeType":"77","messageId":"78","endLine":4,"endColumn":41,"fix":"79"},{"ruleId":"80","severity":1,"message":"81","line":5,"column":8,"nodeType":"82","messageId":"83","endLine":5,"endColumn":14},{"ruleId":"84","severity":1,"message":"85","line":71,"column":17,"nod